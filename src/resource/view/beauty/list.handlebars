<div class="mt-4">
    <h3>
        Danh sách model của tôi
    </h3>
    <table class="table">
    <thead>
        <tr>
        <th scope="col">Số thứ tự</th>
        <th scope="col">Tên ảnh</th>
        <th scope="col">Miêu tả ảnh</th>
        <th scope="col">Link ảnh</th>
        <th scope="col"></th>
        <th scope="col"></th>
        </tr>
    </thead>
    <tbody>
        {{#each course}}
        <tr>
        <th scope="row">{{sum @index 1}}</th>
        <td>{{this.name}}</td>
        <td>{{this.description}}</td>
        <td>{{this.image}}</td>
        <td>   
            <a href="/beauty/{{this._id}}/edit" class="btn btn-link">Sửa</button>
        </td>
        </tr>
        {{/each}}
    </tbody>
    </table>
</div>


